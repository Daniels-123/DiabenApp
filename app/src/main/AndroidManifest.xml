<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dybcatering.diabenapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeRegist">
        <activity android:name="com.dybcatering.diabenapp.Login.HomeActivity"></activity>
        <activity
            android:name="com.dybcatering.diabenapp.Login.RegisterActivity"
            android:theme="@style/AppThemeRegist" />
        <activity
            android:name="com.dybcatering.diabenapp.Login.LoginActivity"
            android:theme="@style/AppThemeLogin" />
        <activity
            android:name="com.dybcatering.diabenapp.Inicio.PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppThemeRegist.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dybcatering.diabenapp.Inicio.SubPrimerActivity"></activity>
    </application>

</manifest>